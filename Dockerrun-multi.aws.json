  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "docka-one",
      "image": "<REPOSITORY_URI>:<IMAGE_TAG>",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 9000
        }
      ]
    }
  ]
}
